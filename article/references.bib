
@article{amundson_hierarchical_2014,
	title = {A hierarchical model combining distance sampling and time removal to estimate detection probability during avian point counts},
	volume = {131},
	issn = {0004-8038, 1938-4254},
	url = {https://academic.oup.com/auk/article/131/4/476-494/5149376},
	doi = {10.1642/AUK-14-11.1},
	abstract = {Imperfect detection during animal surveys biases estimates of abundance and can lead to improper conclusions regarding distribution and population trends. Farnsworth et al. (2005) developed a combined distance-sampling and time-removal model for point-transect surveys that addresses both availability (the probability that an animal is available for detection; e.g., that a bird sings) and perceptibility (the probability that an observer detects an animal, given that it is available for detection). We developed a hierarchical extension of the combined model that provides an integrated analysis framework for a collection of survey points at which both distance from the observer and time of initial detection are recorded. Implemented in a Bayesian framework, this extension facilitates evaluating covariates on abundance and detection probability, incorporating excess zero counts (i.e. zero-inflation), accounting for spatial autocorrelation, and estimating population density. Species-specific characteristics, such as behavioral displays and territorial dispersion, may lead to different patterns of availability and perceptibility, which may, in turn, influence the performance of such hierarchical models. Therefore, we first test our proposed model using simulated data under different scenarios of availability and perceptibility. We then illustrate its performance with empirical point-transect data for a songbird that consistently produces loud, frequent, primarily auditory signals, the Golden-crowned Sparrow (Zonotrichia atricapilla); and for 2 ptarmigan species (Lagopus spp.) that produce more intermittent, subtle, and primarily visual cues. Data were collected by multiple observers along point transects across a broad landscape in southwest Alaska, so we evaluated point-level covariates on perceptibility (observer and habitat), availability (date within season and time of day), and abundance (habitat, elevation, and slope), and included a nested point-withintransect and park-level effect. Our results suggest that this model can provide insight into the detection process during avian surveys and reduce bias in estimates of relative abundance but is best applied to surveys of species with greater availability (e.g., breeding songbirds).},
	language = {en},
	number = {4},
	urldate = {2021-07-15},
	journal = {The Auk},
	author = {Amundson, Courtney L. and Royle, J. Andrew and Handel, Colleen M.},
	month = oct,
	year = {2014},
	pages = {476--494},
	file = {Amundson et al. - 2014 - A hierarchical model combining distance sampling a.pdf:C\:\\Users\\brand\\Zotero\\storage\\56Z4VDGZ\\Amundson et al. - 2014 - A hierarchical model combining distance sampling a.pdf:application/pdf},
}

@article{solymos_phylogeny_2018,
	title = {Phylogeny and species traits predict bird detectability},
	volume = {41},
	issn = {09067590},
	url = {https://onlinelibrary.wiley.com/doi/10.1111/ecog.03415},
	doi = {10.1111/ecog.03415},
	language = {en},
	number = {10},
	urldate = {2021-07-15},
	journal = {Ecography},
	author = {Sólymos, Péter and Matsuoka, Steven M. and Stralberg, Diana and Barker, Nicole K. S. and Bayne, Erin M.},
	month = oct,
	year = {2018},
	keywords = {traits},
	pages = {1595--1603},
	annote = {This paper explores the use of phylogeny and species traits in predicting singing rate and detection distance, both as previously calculated from Solymos et al. (2013). That is, can be use information about species traits and phylogeny to gain better estimates of singing rate and/or detection distance? The authors used 5 species traits here: migration strategy, body mass, habitat associations, minimum sound frequency, and minimum nest height. The authors regressed log(singing rate) and log(detection distance) against these. The authors also used phylogenetic mixed models to estimate \${\textbackslash}lambda\$, the strength of phylogeneitic effects. The authors found that both singing rate and detection distance were in fact correlated to traits and phylogeny, but that strength differed for each one. The combination of phylogeny and species traits best explained singing rate, with phylogeny having the stronger predictive power. They found that long-distant migrants sang at a higher rate than residents. The detection distance was best explained by the traits only model, with large detection distances characterized by larger body mass, migratory species, and species associated with open habitats. The authors conclude that considering traits as somethgn that influences detection distance and singing rate would be a useful tool to be able to group birds to run as a multi-species model, alleviating small sample sizes in some species. That is, in my NA-POPS project, I could group sets of species taht share similar traits, and run that as a multi-species model to share information across those species to allow for data-poor species to shrink toward a mean caused by data-rich species.},
	annote = {This study investigated the effects of bird traits and bird phylogeny on detectability. Most important results were that the traits of song pitch and migratory status appeared to have the greatest effect on a bird's availability (propensity to give a cue), and the traits of song pitch, body size, migratory status, and habitat affinity appeared to have the greatest effect on an observer's ability to perceive the bird. This grouping of detectability based on traits could be a useful way to model detectability of several brids such as in NA-POPS.},
	file = {Sólymos et al. - 2018 - Phylogeny and species traits predict bird detectab.pdf:C\:\\Users\\brand\\Zotero\\storage\\9RFRUHCU\\Sólymos et al. - 2018 - Phylogeny and species traits predict bird detectab.pdf:application/pdf},
}

